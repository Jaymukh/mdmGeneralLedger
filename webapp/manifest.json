{
	"_version": "1.12.0",
	"sap.app": {
		"id": "murphy.mdm.mdmGLAccount",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"CostCenterDataSource": {
				"uri": "model/local.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "murphy.mdm.mdmGLAccount.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.60.38",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "murphy.mdm.mdmGLAccount.i18n.i18n"
				}
			},
			"App": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "CostCenterDataSource",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"userManagementModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"role": [],
					"accountGroups": [],
					"data": {}
				}
			},
			"Dropdowns": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"CommentsModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"AttachmentsModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"AuditLogModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"WorkFlowModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"ChangeRequestsModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"LeftEnabled": false,
					"RightEnabled": false,
					"PageData": [],
					"TotalCount": 0,
					"SelectedPageKey": 1,
					"ChangeRequests": [],
					"Statistics": null,
					"DateFrom": null,
					"DateTo": null,
					"Show": "01",
					"CostCenter": "",
					"ProfitCenter": "",
					"City": "",
					"CompanyCode": "",
					"ReqType": "ALL_REQ"
				}
			},
			"SearchModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"LeftEnabled": false,
					"RightEnabled": false,
					"PageData": [],
					"TotalCount": 0,
					"SelectedPageKey": 1,
					"CostCenters": []
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "murphy.mdm.mdmGLAccount.view",
				"controlAggregation": "pages",
				"controlId": "pageContainer",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "AppRoute",
					"pattern": "",
					"target": [
						"GlSearch"
					]
				}
			],
			"targets": {
				"TargetApp": {
					"viewType": "XML",
					"viewName": "App"
				},
				"GlSearch": {
					"viewType": "XML",
					"viewName": "GlSearch"
				},
				"GlCreate": {
					"viewType": "XML",
					"viewName": "GlCreate"
				},
				"GlChangeRequests": {
					"viewType": "XML",
					"viewName": "GlChangeRequests"
				}
			}
		}
	}
}